    "audit_scripts": [
      "analyze_functions.py",
      "decision_support.py",
      "analyze_function_registry.py",
      "analyze_module_dependencies.py",
      "analyze_documentation.py",
      "analyze_error_handling.py",
      "analyze_documentation_sync.py"
    ],
    "concise_output": true,
    "prioritize_issues": true
  },
  "audit": {
    "include_generated_files": false,
    "include_test_files": true,
    "include_legacy_files": false,
    "max_output_lines": 20,
    "save_detailed_results": true,
    "generate_summary": true,
    "highlight_issues": true
  },
  "ai_collaboration": {
    "concise_output": true,
    "detailed_files": true,
    "actionable_insights": true,
    "context_aware": true,
    "priority_issues": true,
    "integration_mode": true
  },
  "analyze_functions": {
    "moderate_complexity_threshold": 50,
    "high_complexity_threshold": 100,
    "critical_complexity_threshold": 200,
    "min_docstring_length": 10,
    "handler_keywords": [
      "handle",
      "process",
      "validate",
      "check",
      "get",
      "set",
      "save",
      "load",
      "create",
      "update",
      "delete",
      "manage",
      "configure",
      "setup"
    ],
    "test_keywords": [
      "test_",
      "test",
      "check_",
      "verify_",
      "assert_"
    ],
    "critical_functions": [
      "main",
      "run",
      "start",
      "stop",
      "init",
      "setup",
      "validate"
    ]
  },
  "validation": {
    "documentation_coverage_threshold": 80.0,
    "moderate_complexity_warning": 50,
    "high_complexity_warning": 100,
    "critical_complexity_warning": 200,
    "duplicate_function_warning": true,
    "missing_docstring_warning": true,
    "critical_issues_first": true
  },
  "error_handling": {
    "decorator_names": [
      "@handle_errors",
      "handle_errors",
      "error_handler"
    ],
    "exception_base_classes": [
      "MHMError",
      "DataError",
      "ConfigurationError",
      "CommunicationError",
      "ValidationError",
      "AIError",
      "FileOperationError"
    ],
    "error_handler_functions": [
      "handle_file_error",
      "handle_network_error",
      "handle_communication_error",
      "handle_configuration_error",
      "handle_validation_error",
      "handle_ai_error",
      "safe_file_operation"
    ],
    "generic_exceptions": {
      "Exception": "MHMError",
      "ValueError": "ValidationError or DataError",
      "KeyError": "DataError or ConfigurationError",
      "TypeError": "ValidationError or DataError"
    },
    "critical_function_keywords": {
      "file_operations": ["open", "read", "write", "save", "load"],
      "network_operations": ["send", "receive", "connect", "request"],
      "data_operations": ["parse", "serialize", "deserialize", "validate"],
      "user_operations": ["create", "update", "delete", "authenticate"],
      "ai_operations": ["generate", "process", "analyze", "classify"]
    }
  },
  "output": {
    "use_emojis": false,
    "show_progress": false,
    "color_output": false,
    "detailed_reports": false,
    "concise_format": true,
    "priority_indicators": true,
    "action_items": true
  },
  "file_patterns": {
    "python_files": "*.py",
    "documentation_files": "*.md",
